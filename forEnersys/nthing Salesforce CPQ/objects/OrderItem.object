    <validationRules>
        <fullName>RecurringChargeTypeMustHaveBillingType</fullName>
        <active>true</active>
        <description>When Charge Type is Recurring, a Billing Type must be selected.</description>
        <errorConditionFormula>AND(ISPICKVAL(ChargeType__c, &apos;Recurring&apos;), ISPICKVAL(BillingType__c, &apos;&apos;))</errorConditionFormula>
        <errorDisplayField>BillingType__c</errorDisplayField>
        <errorMessage>When Charge Type is Recurring, a Billing Type must be selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_billing_frequency</fullName>
        <active>true</active>
        <description>When Charge Type is &apos;One-Time&apos;, a Billing Frequency value cannot be selected.</description>
        <errorConditionFormula>AND(ISPICKVAL(ChargeType__c, &apos;One-Time&apos;), NOT(ISPICKVAL(BillingFrequency__c, &apos;&apos;)))</errorConditionFormula>
        <errorDisplayField>BillingFrequency__c</errorDisplayField>
        <errorMessage>When Charge Type is &apos;One-Time&apos;, a Billing Frequency value cannot be selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_billing_type</fullName>
        <active>true</active>
        <description>When Charge Type is One-Time or Usage, a Billing Type cannot be selected.</description>
        <errorConditionFormula>OR( 
AND(ISPICKVAL(ChargeType__c, &apos;One-Time&apos;), NOT(ISPICKVAL(BillingType__c, &quot;&quot;))), 
AND(ISPICKVAL(ChargeType__c, &apos;Usage&apos;), NOT(ISPICKVAL(BillingType__c, &quot;&quot;))))</errorConditionFormula>
        <errorDisplayField>BillingType__c</errorDisplayField>
        <errorMessage>When Charge Type is One-Time or Usage, a Billing Type value cannot be selected.</errorMessage>
    </validationRules>
