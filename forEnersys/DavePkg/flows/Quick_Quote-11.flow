    <decisions>
        <name>EMEA_User_or_Americas_User</name>
        <label>EMEA User or Americas User</label>
        <locationX>341</locationX>
        <locationY>16</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>EMEA_User</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>UserSBU.SBU__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>MP - EMEA</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>UserSBU.SBU__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RP - EMEA</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>UserSBU.SBU__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>EAS - EMEA</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Gather_Quote_and_Opportunity_Information_0</targetReference>
            </connector>
            <label>EMEA User</label>
        </rules>
        <rules>
            <name>Americas_User</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>UserSBU.SBU__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RP - Americas</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>UserSBU.SBU__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>MP - Americas</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>UserSBU.SBU__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>EAS - Americas</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Gather_Quote_and_Opportunity_Information</targetReference>
            </connector>
            <label>Americas User</label>
        </rules>
    </decisions>
