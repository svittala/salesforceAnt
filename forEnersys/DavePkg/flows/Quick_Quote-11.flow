    <formulas>
        <name>DefaultOppNameValue</name>
        <dataType>String</dataType>
        <expression>{!AccountName.Name} &amp; &quot;-&quot; &amp; TEXT({!$Flow.CurrentDate})</expression>
    </formulas>
    <formulas>
        <name>DefaultQuoteExpDate</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate} + 30</expression>
    </formulas>
    <formulas>
        <name>DraftRecordType</name>
        <dataType>String</dataType>
        <expression>{!$Label.Quote_Record_Type_Draft}</expression>
    </formulas>
    <formulas>
        <description>Limit the SAD choices to the SADs that match the users SBU</description>
        <name>formulaSADLimits</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL ({!UserSBU.SBU__c}, &apos;MP - EMEA&apos;), TEXT(01),
IF(ISPICKVAL ({!UserSBU.SBU__c}, &apos;RP - EMEA&apos;),  TEXT(02),
IF(ISPICKVAL ({!UserSBU.SBU__c}, &apos;EAS - EMEA&apos;),  TEXT(03),
NULL
)))</expression>
    </formulas>
    <formulas>
        <name>OpportunityRecordID</name>
        <dataType>String</dataType>
        <expression>{!$Label.Opportunity_Record_Type_Repeat}</expression>
    </formulas>
