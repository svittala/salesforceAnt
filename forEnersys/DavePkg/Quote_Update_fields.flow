    <decisions>
        <name>EMEA_User_or_Americas_User</name>
        <label>EMEA User or Americas User</label>
        <locationX>657</locationX>
        <locationY>12</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>EMEA_User</name>
            <conditionLogic>(1 OR 2 OR 3) AND 4</conditionLogic>
            <conditions>
                <leftValueReference>UserSBU.SBU__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>MP - EMEA</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>UserSBU.SBU__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RP - EMEA</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>UserSBU.SBU__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>EAS - EMEA</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>AccountName.SAP_Customer_Id__c</leftValueReference>
                <operator>NotEqualTo</operator>
            </conditions>
            <connector>
                <targetReference>Gather_Quote_and_Opportunity_Information_0</targetReference>
            </connector>
            <label>EMEA User</label>
        </rules>
        <rules>
            <name>Americas_User</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>UserSBU.SBU__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RP - Americas</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>UserSBU.SBU__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>MP - Americas</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>UserSBU.SBU__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>EAS - Americas</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Gather_Quote_and_Opportunity_Information</targetReference>
            </connector>
            <label>Americas User</label>
        </rules>
        <rules>
            <name>EMEA_User_w_o_SAP_Account</name>
            <conditionLogic>(1 OR 2 OR 3) AND 4</conditionLogic>
            <conditions>
                <leftValueReference>UserSBU.SBU__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>MP - EMEA</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>UserSBU.SBU__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>RP - EMEA</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>UserSBU.SBU__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>EAS - EMEA</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>AccountName.SAP_Customer_Id__c</leftValueReference>
                <operator>EqualTo</operator>
            </conditions>
            <connector>
                <targetReference>screenEMEAProspect</targetReference>
            </connector>
            <label>EMEA User w/o SAP Account</label>
        </rules>
    </decisions>
