    <formulas>
        <name>DefaultOppNameValue</name>
        <dataType>String</dataType>
        <expression>{!AccountName.Name} &amp; &quot;-&quot; &amp; TEXT({!$Flow.CurrentDate})</expression>
    </formulas>
    <formulas>
        <name>DefaultQuoteExpDate</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate} + 30</expression>
    </formulas>
    <formulas>
        <name>DraftRecordType</name>
        <dataType>String</dataType>
        <expression>{!$Label.Quote_Record_Type_Draft}</expression>
    </formulas>
    <formulas>
        <name>formulaBusinessArea</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!UserSBU.SBU__c}, &quot;MP - EMEA&quot;), TEXT({!AccountName.MP_Business_Area__c}),
IF(ISPICKVAL({!UserSBU.SBU__c}, &quot;EAS - EMEA&quot;), TEXT({!AccountName.AD_Business_Area__c}),
NULL))</expression>
    </formulas>
    <formulas>
        <name>formulaDistributionChannel</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!UserSBU.SBU__c}, &quot;MP - EMEA&quot;), TEXT({!AccountName.MP_Distribution_Channel__c}),
IF(ISPICKVAL({!UserSBU.SBU__c}, &quot;RP - EMEA&quot;), TEXT({!AccountName.RP_Distribution_Channel__c}),
IF(ISPICKVAL({!UserSBU.SBU__c}, &quot;EAS - EMEA&quot;), TEXT({!AccountName.AD_Distribution_Channel__c}),
NULL)))</expression>
    </formulas>
    <formulas>
        <name>formulaIndustry</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!UserSBU.SBU__c}, &quot;MP - EMEA&quot;), TEXT({!AccountName.MP_Industry__c}),
IF(ISPICKVAL({!UserSBU.SBU__c}, &quot;RP - EMEA&quot;), TEXT({!AccountName.RP_Industry__c}),
IF(ISPICKVAL({!UserSBU.SBU__c}, &quot;EAS - EMEA&quot;), TEXT({!AccountName.AD_Industry__c}),
NULL)))</expression>
    </formulas>
    <formulas>
        <name>formulaLanguage</name>
        <dataType>String</dataType>
        <expression>IF( {!varSAPLanguage.Key__c} = &apos;SCL-EN&apos;, &apos;ENGLISH&apos;,
IF( {!varSAPLanguage.Key__c} = &apos;SCL-DE&apos;, &apos;GERMAN&apos;, 
IF( {!varSAPLanguage.Key__c} = &apos;SCL-NL&apos;, &apos;DUTCH&apos;, 
IF( {!varSAPLanguage.Key__c} = &apos;SCL-FR&apos;, &apos;FRENCH&apos;, 
IF( {!varSAPLanguage.Key__c} = &apos;SCL-IT&apos;, &apos;ITALIAN&apos;,  
IF( {!varSAPLanguage.Key__c} = &apos;SCL-SV&apos;, &apos;SWEDISH&apos;,
&apos;ENGLISH&apos;
))))))</expression>
    </formulas>
    <formulas>
        <description>Limit the SAD choices to the SADs that match the users SBU</description>
        <name>formulaSADLimits</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL ({!UserSBU.SBU__c}, &apos;MP - EMEA&apos;), TEXT(01),
IF(ISPICKVAL ({!UserSBU.SBU__c}, &apos;RP - EMEA&apos;),  TEXT(02),
IF(ISPICKVAL ({!UserSBU.SBU__c}, &apos;EAS - EMEA&apos;),  TEXT(03),
NULL
)))</expression>
    </formulas>
    <formulas>
        <name>OpportunityRecordID</name>
        <dataType>String</dataType>
        <expression>{!$Label.Opportunity_Record_Type_Repeat}</expression>
    </formulas>
