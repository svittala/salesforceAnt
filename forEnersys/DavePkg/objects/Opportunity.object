    <validationRules>
        <fullName>Approved_primary_quote</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 

 IsWon = true ,
 
 NOT(Primary_Quote_approved__c )
)</errorConditionFormula>
        <errorMessage>Your Primary quote must be approved to move this record to WON</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Lost_Competitor</fullName>
        <active>true</active>
        <description>Must add the competitor when an opportunity is set to closed/lost</description>
        <errorConditionFormula>AND( ISPICKVAL(StageName, &apos;Closed Lost&apos;), 
NOT(ISPICKVAL(SBU__c , &apos;EAS&apos;)), 
ISBLANK(Competitor_Lost_To__c) )</errorConditionFormula>
        <errorDisplayField>Competitor_Lost_To__c</errorDisplayField>
        <errorMessage>Please select.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Reason</fullName>
        <active>true</active>
        <description>Select a closed lost or won reason</description>
        <errorConditionFormula>AND(
OR(
ISPICKVAL(StageName, &apos;Closed Won&apos;),
ISPICKVAL(StageName, &apos;Closed Lost&apos;)
), 
ISBLANK(TEXT(Win_Loss_Reason__c))
)</errorConditionFormula>
        <errorDisplayField>Win_Loss_Reason__c</errorDisplayField>
        <errorMessage>Please select.</errorMessage>
    </validationRules>
