            </connector>
            <label>if not null assign to passed sobject</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_DandB_relation_set</name>
        <label>Is DandB relation set</label>
        <locationX>21</locationX>
        <locationY>28</locationY>
        <defaultConnector>
